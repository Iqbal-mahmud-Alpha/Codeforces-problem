#include<bits/stdc++.h>
using namespace std;

int main()
{
    int tc;
    cin>>tc;
    while(tc--)
    {
        string s,ans="";
        cin>>s;
        for(int i=0; i<s.size(); i++)
        {
            if(i%2==0)
            {
                if(s[i]>'a')
                {
                    ans+='a';
                }
                else ans+='b';
            }
            else
            {
                if(s[i]<'z')
                {
                    ans+='z';
                }
                else ans+='y';
            }
        }
        cout<<ans<<endl;
    }
}
