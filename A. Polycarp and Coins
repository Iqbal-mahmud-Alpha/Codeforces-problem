/**---Graph move---**/
int dx[]= {-1, 0, 1,  0, -1, -1,  1, 1};
int dy[]= { 0, 1, 0, -1, -1,  1, -1, 1};
int kx[]= {-2,-1, 1, 2,  2,  1, -1, -2};
int ky[]= { 1, 2, 2, 1, -1, -2, -2, -1};

int digit(int n)
{
    int sum=0;
    while(n)
    {
        int a=n%10;
        sum+=a;
        n/=10;
    }
    return sum;
}
void solved()
{
    int n;
    cin>>n;
    int ans=n/3;
    if(n%3==0 or n%3==1)
    {
        cout<<ans+(n%3)<<' '<<ans<<nl;
    }
    else
    {
        cout<<ans<<' '<<ans+1<<nl;
    }
}

int main()
{
    int tc;
    //int tc=1;
    cin>>tc;
    while(tc--)
    {
        solved();
    }
}
